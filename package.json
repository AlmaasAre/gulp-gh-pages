{
  "name": "gulp-gh-pages",
  "version": "0.4.0",
  "description": "gulp plugin to publish to Github pages (gh-pages branch)",
  "keywords": [
    "git",
    "gulp",
    "gulpplugin",
    "gh-pages",
    "dist",
    "github"
  ],
  "repository": "shinnn/gulp-gh-pages",
  "license": "MIT",
  "author": {
    "name": "Micheal Benedict (@micheal)",
    "email": "micheal@visionmasterdesigns.com",
    "url": "https://github.com/rowoot"
  },
  "contributors": [
    {
      "name": "Shinnosuke Watanabe",
      "url": "https://github.com/shinnn"
    }
  ],
  "files": [
    "index.js",
    "lib"
  ],
  "scripts": {
    "pretest": "jscs *.js lib/*.js && eslint *.js lib/*.js",
    "test": "_mocha test.js --timeout 10000",
    "test-appveyor": "_mocha test.js --timeout 20000 --reporter dot",
    "coverage": "istanbul cover _mocha test.js -x=test.js -- --timeout 10000",
    "coveralls": "${npm_package_scripts_coverage} && istanbul-coveralls"
  },
  "dependencies": {
    "es6-promise": "^2.0.1",
    "gift": "^0.5.0",
    "gulp-util": "^3.0.4",
    "rimraf": "^2.2.6",
    "through2": "^0.6.1",
    "vinyl-fs": "^1.0.0",
    "wrap-promise": "^1.0.0"
  },
  "devDependencies": {
    "eslint": "^0.15.1",
    "graceful-fs": "^3.0.5",
    "istanbul": "^0.3.6",
    "istanbul-coveralls": "^1.0.2",
    "jscs": "^1.11.3",
    "log-symbols": "^1.0.2",
    "mocha": "^2.1.0",
    "node-uuid": "^1.4.2",
    "octonode": "^0.6.15",
    "read-remove-file": "^1.0.1",
    "vinyl": "^0.4.6"
  },
  "jscsConfig": {
    "preset": "google",
    "maximumLineLength": 98,
    "requireBlocksOnNewline": true,
    "requireCamelCaseOrUpperCaseIdentifiers": "ignoreProperties",
    "validateLineBreaks": "LF"
  }
}
